<table class="table is-fullwidth">
  <thead>
    <th>Stars</th>    
    <th>User</th>
    <th>Category</th>
  </thead>
  <tbody>
    {{#each reviews}}
      <tr>
        <td>{{stars}}</td>        
        <td> {{donor.firstName}} {{donor.lastName}} </td>
        <td> {{category.title}} </td>
        <td> <img width="300" src={{category.img}}> </td> 
      </tr>
    {{/each}}
  </tbody>
</table>